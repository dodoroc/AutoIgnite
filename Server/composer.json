{
  "name": "autoignite/server",
  "description": "Simple server to store and find missing episodes for a monitored series",
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
    "name": "vjr aka dodoroc"
    }
  ],

  "require": {
    "php": ">=8.3.6"
  },

  "config": {
    "optimize-autoloader": true
  },

  "autoload": {
    "psr-4": {
      "Server\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
